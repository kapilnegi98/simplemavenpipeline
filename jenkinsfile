pipeline {

    agent any
    tools {
        maven 'apache-maven-3.6.3' 
    }
    stages {
        stage('Compile stage') {
            steps {
            script{
                 app = docker.build("kapilnegi/java-maven")
        }
        }
    }
    stage('Compile stage') {
            steps {
                docker.withRegistry('https://registry.hub.docker.com', 'dockerhub') {
            app.push("${env.BUILD_NUMBER}")
            app.push("latest")
            } 
        }
    }
    

   
  }

}
