pipeline {

    agent any
    tools {
        maven 'apache-maven-3.6.3' 
    }
    stages {
        stage('Compile stage') {
            steps {
                bat "mvn clean compile" 
        }
    }

         stage('testing stage') {
             steps {
                bat "mvn test"
        }
    }

          stage('deployment stage') {
              steps {
                bat "mvn clean"
        }
    }
    stage('Sonarqube analysis') {
    steps {
    script {
             scannerHome = tool 'sonar scanner';
        }
     withSonarQubeEnv('SonarQube') {
      
    }

    }
        }

  }

}
